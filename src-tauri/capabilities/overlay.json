{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "overlay",
  "description": "Capabilities for the overlay windows",
  "windows": [
    "*"
  ],
  "remote": {
    "urls": [
      "http://localhost:9527"
    ]
  },
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "store:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "fs:default",
    "opener:default",
    "dialog:default"
  ]
}