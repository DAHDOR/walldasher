{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "overlay",
  "description": "Capability for the overlay window",
  "windows": [
    "overlay"
  ],
  "remote": {
    "urls": [
      "http://localhost:9527"
    ]
  },
  "permissions": [
    "allow-tournaments",
    "allow-tournament",
    "allow-bracket-matches",
    "allow-event-players",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "store:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "websocket:default"
  ]
}